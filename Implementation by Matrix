#AdjMatrix(Directed graph)
import sys
from pprint import pprint

vertices = int(sys.stdin.readline().rstrip())
edges = int(sys.stdin.readline().rstrip())
adj_mat = [[0] * vertices for i in range(vertices)]


for i in range(edges):
    a, b = map(int, sys.stdin.readline().split())
    adj_mat[a][b] = 1
    adj_mat[b][a] = 1

pprint(adj_mat)

#AdjMatrix(Undirected graph)

vertices = int(sys.stdin.readline().rstrip())
edges = int(sys.stdin.readline().rstrip())
adj_mat = [[0] * vertices for i in range(vertices)]


for i in range(edges):
    a, b = map(int, sys.stdin.readline().split())
    adj_mat[a][b] = 1

pprint(adj_mat)
