class Arraystack:
    
    def __init__(self):
        self.data = []
        
    def __repr__(self):
        if self.isEmpty():
            return "Stack is empty!"
        message = ""
        
        for item in reversed(self.data):
            message += str(item)
            if item != self.data[0]:
                message += '\n'
                
        return message
    
    def size(self):
        return len(self.data)
    
    def isEmpty(self):
        return self.size() == 0
    
    def peek(self):
        if self.size() == 0:
            return None
        
        return self.data[-1]
    
    def push(self, item):
        self.data.append(item)
        return True
    
    def pop(self):
        if self.isEmpty():
            return None
        
        return self.data.pop()
